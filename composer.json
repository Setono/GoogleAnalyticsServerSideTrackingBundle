{
    "name": "setono/google-analytics-server-side-tracking-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that allows you to track your visitors server side instead of client side",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "gedmo/doctrine-extensions": "^2.4",
        "nyholm/psr7": "^1.3",
        "setono/google-analytics-measurement-protocol": "^1.0@dev",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.1.5",
        "symfony/uid": "^5.1",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "kriswallsmith/buzz": "^1.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "nyholm/symfony-bundle-test": "^1.7",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^2.1",
        "roave/security-advisories": "dev-master",
        "setono/code-quality-pack": "^2.0",
        "weirdan/doctrine-psalm-plugin": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\GoogleAnalyticsServerSideTrackingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\GoogleAnalyticsServerSideTrackingBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": [
            "phpstan analyse",
            "psalm"
        ],
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpspec": "phpspec run",
        "phpunit": "phpunit"
    }
}
